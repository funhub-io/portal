<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="disconfMgrBean" class="com.baidu.disconf.client.DisconfMgrBean"
          destroy-method="destroy">
        <property name="scanPackage" value="com.netease.cloud.ncegdashboard"/>
    </bean>

    <bean id="disconfMgrBean2" class="com.baidu.disconf.client.DisconfMgrBeanSecond"
          init-method="init" destroy-method="destroy">
    </bean>

    <context:component-scan base-package="com.netease.cloud.gdashboard.api"/>

    <bean id="cipherKey" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="111111111111111111111111"/>
    </bean>

    <bean id="sessionCipherKey" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="session.key"/>
    </bean>
</beans>